<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<HBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.jenhan.wowfeatureextractiontool.Gui">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="mainWindow"/>
    <Button text="install addon" onAction="#onInstallAddonClick"/>
    <Button text="selectFolder" onAction="#onSelectFolderClick"/>
    <Button text="export XML" onAction="#onExportToXmlClick"/>
</HBox>
