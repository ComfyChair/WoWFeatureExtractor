<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<DialogPane fx:id="sessionSelectionParent"
            maxHeight="1.7976931348623157E308"
            maxWidth="1.7976931348623157E308"
            prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.jenhan.wowfeatureextractiontool.MainControl">
   <content>
      <ScrollPane >
         <content>
            <ListView fx:id="sessionSelectionListView" prefHeight="400.0" prefWidth="593.0" />
         </content>
      </ScrollPane>
      <HBox alignment="CENTER_RIGHT" prefHeight="54.0" prefWidth="580.0" >
         <children>
            <Button mnemonicParsing="false" onAction="#selectSession" text="ok">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="cancel" />
         </children>
         <opaqueInsets>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </opaqueInsets>
      </HBox>
   </content>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</DialogPane>
