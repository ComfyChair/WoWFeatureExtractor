<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<Pane maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.jenhan.wowfeatureextractiontool.SessionSelectionController">
       <TableView fx:id="sessionInfoTable" prefHeight="400.0" prefWidth="640.0">
           <columns>
               <TableColumn fx:id="sessionID" prefWidth="75.0" text="ID" />
               <TableColumn fx:id="dateTime" prefWidth="75.0" text="Date / Time" />
               <TableColumn fx:id="charName" prefWidth="75.0" text="Character" />
               <TableColumn fx:id="serverName" prefWidth="75.0" text="Server" />
           </columns>
           <columnResizePolicy>
               <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
           </columnResizePolicy>
       </TableView>
</Pane>
